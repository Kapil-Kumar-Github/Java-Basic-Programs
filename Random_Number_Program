import java.util.*;
public class Random_Number {
    public static void main(String [] args){
        double d = Math.random() ;
        int max = 400 ;
        int min = 100 ;
        int  a = (int) (Math.random() * (max-min +1) + min);
        System.out.println(d) ;
        System.out.println("Random numbers between 100 and 400 be " + a) ;

    }
}
