public class Armstrong {
    static int   sum = 0;
   static int Armstrong_Number(String a ){
       int n1=0 ; ;
        for(int i = 0; i<a.length() ; i++){
            char n = a.charAt(i) ;
             n1= n - '0' ;
          sum = sum + (int) Math.pow(n1 , a.length()) ;

            //System.out.println("Length be " + (2)^(a.length())) ;
        }

        return sum;
    }
    public static void main(String [] args){
     System.out.println(Armstrong_Number("75")) ;



    }

}
